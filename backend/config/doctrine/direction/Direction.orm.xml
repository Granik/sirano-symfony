<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Domain\Entity\Direction\Direction" table="directions">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>

        <field name="name" type="string" />
        <field name="icon" type="string" nullable="true"/>
        <field name="activeIcon" type="string" nullable="true"/>
        <field name="image" type="string" nullable="true"/>
        <field name="isActive" type="boolean" />
        <field name="isMainPage" type="boolean" />
        <field name="number" type="integer" nullable="true" />
    </entity>
</doctrine-mapping>